<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SuperTextView">
        <attr name="dynamicText" format="string"/>
        <attr name="duration" format="integer"/>
        <attr name="selectedColor" format="color"/>
        <attr name="dynamicStyle" format="enum">
            <enum name="normal" value="0"/>
            <enum name="typewriting" value="1"/>
            <enum name="changeColor" value="2"/>
        </attr>
    </declare-styleable>

<!--    <attr name="selectedColor" format="color"/>  <attr name="duration" format="integer"/>-->
    <attr name="showPainter" format="boolean"/>
    <attr name="showPainterActually" format="boolean"/>
    <attr name="pathStrokeWidth" format="dimension"/>
    <attr name="pathStrokeColor" format="color"/>
    <attr name="paintStrokeWidth" format="dimension"/>
    <attr name="paintStrokeColor" format="color"/>
    <attr name="fillColor" format="color"/>
    <attr name="repeat" format="enum">
        <enum name="RESTART" value="1"/>
        <enum name="REVERSE" value="2"/>
    </attr>

    <declare-styleable name="TextPathView">
        <attr name="textSize" format="dimension"/>
        <attr name="text" format="string"/>
        <attr name="autoStart" format="boolean"/>
        <attr name="showInStart" format="boolean"/>
        <attr name="textInCenter" format="boolean"/>
        <attr name="duration" />
        <attr name="showPainter" />
        <attr name="showPainterActually" />
        <attr name="pathStrokeWidth" />
        <attr name="pathStrokeColor" />
        <attr name="paintStrokeWidth" />
        <attr name="paintStrokeColor" />
        <attr name="repeat" />
    </declare-styleable>

    <declare-styleable name="PathView">
        <attr name="duration" />
        <attr name="showPainter" />
        <attr name="showPainterActually" />
        <attr name="pathStrokeWidth" />
        <attr name="pathStrokeColor" />
        <attr name="paintStrokeWidth" />
        <attr name="paintStrokeColor" />
        <attr name="repeat" />
    </declare-styleable>

    <declare-styleable name="SwipeBackLayout">
        <attr name="swipeBackFactor" format="float"></attr>
        <attr name="maskAlpha" format="integer"></attr>
        <attr name="isSwipeFromEdge" format="boolean"></attr>
        <attr name="directionMode">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="4" />
            <enum name="bottom" value="8" />
        </attr>
    </declare-styleable>

    <attr name="SwipeBackLayoutStyle" format="reference"/>

    <declare-styleable name="CustomCancelView">
        <attr name="view_radius" format="dimension" />
        <attr name="view_color" format="color" />
    </declare-styleable>

    <declare-styleable name="WindowImageView">
        <attr name="src" format="reference" />
        <attr name="frescoEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InsLoadingViewAttr">
        <attr name="rotate_duration" format="integer" />
        <attr name="circle_duration" format="integer" />
        <attr name="start_color" format="color" />
        <attr name="end_color" format="color" />
        <attr name="status" >
            <enum name="loading" value="0" />
            <enum name="clicked" value="1" />
            <enum name="unclicked" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="radius" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="LottieTabView">
        <attr name="text_selected_color" format="color"/>
        <attr name="text_normal_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="lottie_path" format="string"/>
        <attr name="icon_normal" format="reference"></attr>
        <attr name="tab_selected" format="boolean"></attr>
        <attr name="tab_name" format="string"/>
    </declare-styleable>

    <declare-styleable name="NiceImageView">
        <attr name="is_circle" format="boolean" />
        <attr name="is_cover_src" format="boolean" />
        <attr name="corner_radius" format="dimension" />
        <attr name="corner_top_left_radius" format="dimension" />
        <attr name="corner_top_right_radius" format="dimension" />
        <attr name="corner_bottom_left_radius" format="dimension" />
        <attr name="corner_bottom_right_radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="inner_border_width" format="dimension" />
        <attr name="inner_border_color" format="color" />
        <attr name="mask_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LiquidSwipeViewPager">
        <attr name="scrollerDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="FlipTab">
        <attr name="removeDefaultPadding" format="boolean" />
        <attr name="flipAnimationDuration" format="integer" />
        <attr name="wobbleReturnAnimationDuration" format="integer" />
        <attr name="wobbleAngle" format="float" />
        <attr name="overallColor" format="color" />
        <attr name="highlightColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="leftTabText" format="string" />
        <attr name="rightTabText" format="string" />
        <attr name="startingTab" format="enum" >
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="borderWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="BlurImageSwitcher">
        <attr name="scaleFactor" format="float" />
        <attr name="blurFactor" format="float" />
        <attr name="animationDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="LadderTextView">
        <attr name="textContent" format="string"/>
        <attr name="offsetScale" format="float"/>
        <attr name="isLeft" format="boolean"/>
        <attr name="isSelected" format="boolean"/>
        <attr name="selectedColor2" format="color|reference"/>
        <attr name="strokeWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="slideruler">
        <!--最小值-->
        <attr name="min_value" format="integer"/>
        <!--最大值-->
        <attr name="max_value" format="integer"/>
        <!--当前值-->
        <attr name="current_value" format="integer"></attr>
        <!--最小单位值-->
        <attr name="min_unitValue" format="integer"></attr>
        <!--最小当前值-->
        <attr name="min_currentValue" format="integer"></attr>
        <!--数值字体大小-->
        <attr name="textSize2" format="dimension"></attr>
        <!---数值字体颜色-->
        <attr name="textColor2" format="integer"></attr>
        <!--底部和刻度的线颜色-->
        <attr name="dividerColor" format="integer"></attr>
        <!--指示器的颜色-->
        <attr name="indicatrixColor" format="integer"></attr>
        <!--一屏显示Item-->
        <attr name="show_itemSize" format="integer" ></attr>
        <!--刻度和数值的间距-->
        <attr name="margin_cursor_data" format="dimension"></attr>
        <!--长指针大小-->
        <attr name="longCursor" format="dimension"></attr>
        <!--短指针大小-->
        <attr name="shortCursor" format="dimension"></attr>
    </declare-styleable>
</resources>